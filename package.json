{
  "private": true,
  "name": "minimal-blog",
  "description": "Typography driven, feature-rich blogging theme with minimal aesthetics. Includes tags/categories support and extensive features for code blocks such as live preview, line numbers, and line highlighting.",
  "version": "1.3.10",
  "author": "LekoArts <hello@devpanther.de>",
  "license": "0BSD",
  "starter-name": "gatsby-starter-minimal-blog",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "develop:cypress": "cross-env CYPRESS_SUPPORT=y yarn develop",
    "build:cypress": "cross-env CYPRESS_SUPPORT=y yarn build",
    "start": "bash clean-and-run.sh && gatsby develop -p 8900",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "bash clean-and-run.sh && gatsby build && gh-pages -d public -b main"
  },
  "dependencies": {
    "@devpanther/gatsby-theme-minimal-blog": "^2.8.0",
    "@devpanther/gatsby-theme-minimal-blog-core": "^2.7.9",
    "@fontsource/open-sans": "^4.4.2",
    "eslint": "^7.32.0",
    "fs-extra": "^10.0.0",
    "gatsby": "^2.25.3",
    "gatsby-plugin-gatsby-cloud": "^1.0.1",
    "gatsby-plugin-google-analytics": "^2.4.1",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-manifest": "^2.5.2",
    "gatsby-plugin-netlify": "^2.4.0",
    "gatsby-plugin-offline": "^3.3.3",
    "gatsby-plugin-sharp": "^3.13.0",
    "gatsby-plugin-sitemap": "^2.5.1",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.13",
    "gatsby-remark-images": "^5.10.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "eslint-plugin-mdx": "^1.15.0",
    "gh-pages": "^3.2.0"
  }
}
